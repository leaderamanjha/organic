<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="categoriesRowModel" type="com.amankumarsapplication.app.modules.categories.data.model.CategoriesRowModel"/>
    </data>
    <FrameLayout android:id="@+id/frameStackfruitsandvegi"
        android:layout_height="wrap_content" android:layout_width="match_parent">
        <LinearLayout android:id="@+id/linearRowfruitsandvegi"
            android:layout_gravity="top|start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_46pxv"
            android:layout_marginEnd="@dimen/_13pxh"
            android:layout_marginStart="@dimen/_13pxh"
            android:layout_marginTop="@dimen/_46pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:id="@+id/txtFRUITSANDVEGI"
                android:layout_height="wrap_content"
                android:layout_weight="0.5" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_2pxh"
                android:text="@{categoriesRowModel.txtFRUITSANDVEGI}"
                style="@style/txtPoppinsbold16_1" tools:text="@string/msg_fruits_and_vegi"/>
            <TextView android:id="@+id/txtFRUITSANDVEGIOne"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_80pxh"
                android:layout_weight="0.5" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_2pxh"
                android:text="@{categoriesRowModel.txtFRUITSANDVEGIOne}"
                style="@style/txtPoppinsbold16_1" tools:text="@string/msg_fruits_and_vegi"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearColumn"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_width="wrap_content"
            android:orientation="vertical" style="@style/groupStylegray_101">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnfavorite"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                <ImageView android:id="@+id/imageFavoriteOne1"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_19pxh"
                    android:layout_marginEnd="@dimen/_6pxh"
                    android:layout_marginStart="@dimen/_122pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="@dimen/_30pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_favorite}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_favorite"/>
                <ImageView android:id="@+id/imageRectangleEighteen"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_86pxh"
                    android:layout_marginEnd="@dimen/_29pxh"
                    android:layout_marginStart="@dimen/_29pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="@dimen/_100pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_27pxh}"
                    imageRes="@{@drawable/img_rectangle18}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle18"/>
                <FrameLayout android:id="@+id/frameStacklanguage"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_36pxv"
                    android:layout_marginEnd="@dimen/_29pxh"
                    android:layout_marginStart="@dimen/_29pxh"
                    android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_97pxh">
                    <TextView android:gravity="top|center"
                        android:id="@+id/txtLanguage"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:text="@{categoriesRowModel.txtLanguage}"
                        style="@style/txtPoppinslight12" tools:text="@string/msg_milk_medium_fat"/>
                    <ImageView android:id="@+id/imageRupee"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginEnd="@dimen/_21pxh"
                        android:layout_marginStart="@dimen/_21pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_rupee"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rupee"/>
                </FrameLayout>
                <FrameLayout
                    android:id="@+id/frameStackrectanglenineteen"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_18pxv"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginEnd="@dimen/_29pxh"
                    android:layout_marginStart="@dimen/_29pxh"
                    android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_95pxh">
                    <View
                        android:background="@drawable/rectangle_bg_green_700_radius_9"
                        android:id="@+id/viewRectangleNineteen"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_18pxv"
                        android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_95pxh"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtADD"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginEnd="@dimen/_35pxh"
                        android:layout_marginStart="@dimen/_34pxh"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:singleLine="true"
                        android:text="@{categoriesRowModel.txtADD}"
                        style="@style/txtPoppinsmedium12_2" tools:text="@string/lbl_add"/>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearColumnOne"
            android:layout_gravity="end"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_width="wrap_content"
            android:orientation="vertical" style="@style/groupStylegray_101">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnrectangleeighteenOne"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearRowrectangleeighteenOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_6pxh"
                    android:layout_marginStart="@dimen/_45pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView
                        android:id="@+id/imageRectangleEighteenOne"
                        android:layout_height="@dimen/_104pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="@dimen/_68pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_27pxh}"
                        imageRes="@{@drawable/img_rectangle18_104x68}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle18_104x68"/>
                    <ImageView android:id="@+id/imageFavoriteOne2"
                        android:layout_height="@dimen/_19pxh"
                        android:layout_marginBottom="@dimen/_98pxv"
                        android:layout_marginStart="@dimen/_9pxh"
                        android:layout_width="@dimen/_30pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_favorite}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_favorite"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameStacklanguageOne"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_36pxv"
                    android:layout_marginEnd="@dimen/_31pxh"
                    android:layout_marginStart="@dimen/_31pxh"
                    android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_69pxh">
                    <TextView android:gravity="top|center"
                        android:id="@+id/txtLanguageOne"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:text="@{categoriesRowModel.txtLanguageOne}"
                        style="@style/txtPoppinslight12" tools:text="@string/msg_milk_bottle"/>
                    <ImageView android:id="@+id/imageRupeeOne"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_rupee"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rupee"/>
                </FrameLayout>
                <FrameLayout
                    android:id="@+id/frameStackrectanglenineteenOne"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_18pxv"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginEnd="@dimen/_31pxh"
                    android:layout_marginStart="@dimen/_31pxh"
                    android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_95pxh">
                    <View
                        android:background="@drawable/rectangle_bg_green_700_radius_9"
                        android:id="@+id/viewRectangleNineteenOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_18pxv"
                        android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_95pxh"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtADDOne"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginEnd="@dimen/_35pxh"
                        android:layout_marginStart="@dimen/_34pxh"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:singleLine="true"
                        android:text="@{categoriesRowModel.txtADDOne}"
                        style="@style/txtPoppinsmedium12_2" tools:text="@string/lbl_add"/>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
